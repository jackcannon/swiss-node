{
  "name": "swiss-node",
  "version": "1.3.0",
  "author": "Jack Cannon <jackc@annon.co.uk> (http://c.annon.co.uk/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jackcannon/swiss-node.git"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "@types/node": "^18.11.15",
    "swiss-docs": "^0.3.0",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "build": "yarn build:code && yarn docs",
    "build:code": "tsup-node src/index.ts --format cjs,esm --dts --clean",
    "watch": "yarn build -- --watch src",
    "docs": "swiss-docs -i src -o README.md",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "fuse.js": "^6.6.2",
    "prompts": "^2.4.2",
    "string-width": "^5.1.2",
    "swiss-ak": "^2.2.1"
  }
}
